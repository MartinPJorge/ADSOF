package es.uam.eps.adsof.batalla5ejercitos.criaturas;

/**
 * Esta clase, que extiende la clase abstracta CriaturaOscura, está asociada a
 * la raza Huargo, participante en la Batalla de los 5 Ejércitos.
 *
 * @author Iv&aacute;n M&aacute;rquez Pardo
 * @author Jorge Mart&iacute;n P&eacute;rez
 * @version 1.0
 */
public class Huargo extends CriaturaOscura {

    /**
     * Constructor de la clase Huargo.
     *
     * @param ptosVida
     * @param ataque
     * @param defensa
     * @param heridas
     */
    public Huargo(int ptosVida, int ataque, int defensa, int heridas) {
        super(ptosVida, ataque, defensa, heridas);
    }
}
